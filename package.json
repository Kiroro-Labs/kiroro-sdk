{
    "name": "@kirorolabs/sdk",
    "version": "2.1.1",
    "description": "KiroroLabs Social-Native Auth & Web3 SDK for Threads + Multi-Chain",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./wagmi": {
            "types": "./dist/wagmi.d.ts",
            "import": "./dist/wagmi.mjs",
            "require": "./dist/wagmi.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.tsx src/wagmi.tsx --format cjs,esm --external react --external react-dom --external wagmi --external viem",
        "build:dts": "tsup src/index.tsx src/wagmi.tsx --format cjs,esm --dts --external react --external react-dom --external wagmi --external viem",
        "watch": "tsup src/index.tsx src/wagmi.tsx --format cjs,esm --dts --external react --external react-dom --external wagmi --external viem --watch",
        "lint": "eslint src",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "threads",
        "auth",
        "privy",
        "gasless",
        "base",
        "web3",
        "social",
        "kiroro",
        "embedded-wallet",
        "wagmi",
        "viem",
        "smart-wallet",
        "erc-4337",
        "account-abstraction"
    ],
    "author": "Kiroro Labs",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Kiroro-Labs/kiroro-sdk"
    },
    "homepage": "https://kiroro.xyz",
    "dependencies": {
        "@privy-io/react-auth": "^1.93.0",
        "@solana/web3.js": "^1.98.4",
        "@wagmi/core": "^2.0.0"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18",
        "viem": ">=2.0.0",
        "wagmi": ">=2.0.0"
    },
    "peerDependenciesMeta": {
        "wagmi": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.3",
        "viem": "^2.0.0",
        "wagmi": "^2.0.0"
    }
}